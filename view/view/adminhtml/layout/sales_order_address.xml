<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="head.additional">
            <block
                    class="Afd\Pce\Block\Adminhtml\Css"
                    name="afd.admin.css"
                    before="-"
            >
                <arguments>
                    <argument name="page" xsi:type="string">adminOrder</argument>
                </arguments>
            </block>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Afd\Pce\Block\Afd" name="afd_options" template="Afd_Pce::afd_options.phtml" ifconfig="afd_typeahead/forms/adminOrder"/>
            <block class="Afd\Pce\Block\Afd" name="afd_phone" template="Afd_Pce::phone.phtml" ifconfig="afd_phone/enable/enable"/>
            <block class="Afd\Pce\Block\Afd" name="afd_admin_order_edit_typeahead" template="Afd_Pce::order/address/admin_order_edit_typeahead.phtml" after="afd_options" ifconfig="afd_typeahead/forms/adminOrder"/>
            <block class="Afd\Pce\Block\Afd" name="afd_admin_order_edit_phone" template="Afd_Pce::order/address/admin_order_edit_phone.phtml" after="afd_options" ifconfig="afd_phone/forms/adminOrder"/>
        </referenceContainer>
    </body>
</page>
